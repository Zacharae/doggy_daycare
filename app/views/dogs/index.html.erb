<h1>Dogs#index</h1>

<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Name</th>
      <th>Breed</th>
      <th>Age</th>
    </tr>
  </thead>
  <tbody>
    <% @dogs.each do |dog| %>
    <tr>
      <td><%= dog.id %></td>
      <td><%= dog.name %></td>
      <td><%= dog.breed %></td>
      <td><%= dog.age %></td>
      <td><%= link_to "show dog", dog_path(dog) %></td>
    </tr>
    <% end %>
  </tbody>
</table>


<%= link_to "add a dog", new_dog_path %>
<br>
<a href="/dog/daycare">home</a>